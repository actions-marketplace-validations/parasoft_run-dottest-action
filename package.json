    "compile": "tsc -p ./ && ncc build",
    "watch": "tsc -watch -p ./",
    "lint": "eslint --ext .ts .",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} mocha -u tdd -r ts-node/register 'tests/**/*.ts'"
  },
